{
  "name": "NOON",
  "description": "NOON chrome extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "js": [
        "popup.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}